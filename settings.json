{
  "SlackNotifications": 
  {
    "AzureQueue": 
    {
      "ConnectionString": "${SlackConnString}",
      "QueueName": "${SlackQueueName}"
    },
    "ThrottlingLimitSeconds": "${SlackThrottlingLimitSeconds}"
  },
  "SmsSenderService": 
  {
    "BaseUrl": "${SmsSenderServiceBaseUrl}",
    "Db": 
    {
      "LogsConnString": "${SmsSenderServiceLogsConnStr}",
      "DataConnString": "${SmsSenderServiceDataConnStr}"
    },
    "Rabbit": 
    {
      "ConnectionString": "${SmsSenderServiceRabbitConnStr}"
    },
    "SmsSettings": 
    {
      "DefaultSmsProvider": "${SmsServiceDefaultSmsProvider}",
      "SmsRetryTimeout": "${SmsServiceSmsRetryTimeout}"
    },
    "Senders": 
    {
      "Twilio": 
      {
        "BaseUrl": "${TwilioBaseUrl}",
        "ApiKey": "${TwilioApiKey}",
        "ApiSecret": "${TwilioApiSecret}",
        "From": "{TwilioFrom}",
        "FromMap": 
        {
        }
      },
      "Nexmo": 
      {
        "BaseUrl": "${NexmoBaseUrl}",
        "ApiKey": "${NexmoApiKey}",
        "ApiSecret": "${NexmoApiSecret}",
        "From": "${NexmoFrom}",
        "FromMap": 
        {
          "US": "${NexmoFromMapUS}"
        }
      },
      "Routee": 
      {
        "BaseUrl": "${RouteeBaseUrl}",
        "AuthUrl": "${RouteeAuthUrl}",
        "ApiKey": "${RouteeApiKey}",
        "ApiSecret": "${RouteeApiSecret}",
        "From": "${RouteeFrom}",
        "FromMap": 
        {
        }
      }
    }
  }
}
